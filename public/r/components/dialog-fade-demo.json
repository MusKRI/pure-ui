      "content": "import {\r\n  Dialog,\r\n  DialogPopup,\r\n  DialogDescription,\r\n  DialogFooter,\r\n  DialogHeader,\r\n  DialogTitle,\r\n  DialogTrigger,\r\n} from \"@/registry/default/components/ui/dialog\";\r\n\r\nimport { Button } from \"@/registry/default/components/ui/button\";\r\n\r\nexport const DialogFadeDemo = () => {\r\n  return (\r\n    <Dialog>\r\n      <DialogTrigger render={<Button />}>Open</DialogTrigger>\r\n\r\n      <DialogPopup animationPreset=\"fade\">\r\n        <DialogHeader>\r\n          <DialogTitle>Terms of Service</DialogTitle>\r\n          <DialogDescription>\r\n            Please read the following terms of service carefully.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n\r\n        <div className=\"grid gap-4 py-4\">\r\n          <p>\r\n            Lorem ipsum dolor sit amet consectetur adipisicing elit. Quisquam,\r\n            quos. Lorem ipsum dolor sit amet consectetur adipisicing elit.\r\n            Quisquam, quos.\r\n          </p>\r\n        </div>\r\n\r\n        <DialogFooter>\r\n          <Button variant=\"outline\" radius=\"full\" className=\"w-full sm:w-auto\">\r\n            Decline\r\n          </Button>\r\n          <Button type=\"submit\" radius=\"full\" className=\"w-full sm:w-auto\">\r\n            Accept\r\n          </Button>\r\n        </DialogFooter>\r\n      </DialogPopup>\r\n    </Dialog>\r\n  );\r\n};\r\n",
      "type": "registry:example"
    }
  ]
