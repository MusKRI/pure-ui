      "content": "import {\r\n  Dialog,\r\n  DialogPopup,\r\n  DialogDescription,\r\n  DialogFooter,\r\n  DialogHeader,\r\n  DialogTitle,\r\n  DialogTrigger,\r\n  DialogClose,\r\n} from \"@/registry/default/components/ui/dialog\";\r\n\r\nimport { Button } from \"@/registry/default/components/ui/button\";\r\nimport { Input } from \"@/registry/default/components/ui/input\";\r\n\r\nexport const DialogDemo = () => {\r\n  return (\r\n    <Dialog>\r\n      <DialogTrigger asChild>\r\n        <Button>Open parent</Button>\r\n      </DialogTrigger>\r\n      <DialogPopup showCloseButton={false}>\r\n        <DialogHeader>\r\n          <DialogTitle>Manage team member</DialogTitle>\r\n          <DialogDescription>\r\n            View and manage a user in your team.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n        <div className=\"grid gap-4\">\r\n          <div className=\"grid gap-1\">\r\n            <p className=\"text-sm text-muted-foreground\">Name</p>\r\n            <p className=\"text-sm font-medium\">Bora Baloglu</p>\r\n          </div>\r\n          <div className=\"grid gap-1\">\r\n            <p className=\"text-sm text-muted-foreground\">Email</p>\r\n            <p className=\"text-sm font-medium\">bora@example.com</p>\r\n          </div>\r\n        </div>\r\n        <DialogFooter>\r\n          <Dialog>\r\n            <DialogTrigger asChild>\r\n              <Button variant=\"outline\">Edit details</Button>\r\n            </DialogTrigger>\r\n            <DialogPopup showCloseButton={false}>\r\n              <DialogHeader>\r\n                <DialogTitle>Edit details</DialogTitle>\r\n                <DialogDescription>\r\n                  Make changes to the member&apos;s information.\r\n                </DialogDescription>\r\n              </DialogHeader>\r\n              <div className=\"flex flex-col gap-4\">\r\n                <Input />\r\n                <Input />\r\n              </div>\r\n              <DialogFooter>\r\n                <DialogClose render={<Button variant=\"ghost\" />}>\r\n                  Cancel\r\n                </DialogClose>\r\n                <Button type=\"submit\">Save changes</Button>\r\n              </DialogFooter>\r\n            </DialogPopup>\r\n          </Dialog>\r\n        </DialogFooter>\r\n      </DialogPopup>\r\n    </Dialog>\r\n  );\r\n};\r\n",
      "type": "registry:example"
    }
