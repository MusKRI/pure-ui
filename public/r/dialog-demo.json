      "path": "src/registry/pure-ui/examples/dialog/dialog-demo.tsx",
      "content": "import {\r\n  Dialog,\r\n  DialogPopup,\r\n  DialogDescription,\r\n  DialogFooter,\r\n  DialogHeader,\r\n  DialogTitle,\r\n  DialogTrigger,\r\n  DialogClose,\r\n} from \"@/registry/pure-ui/ui/dialog\";\r\n\r\nimport { Button } from \"@/registry/pure-ui/ui/button\";\r\nimport { Input } from \"@/registry/pure-ui/ui/input\";\r\nimport { Label } from \"@/registry/pure-ui/ui/label\";\r\n\r\nexport const DialogDemo = () => {\r\n  return (\r\n    <Dialog>\r\n      <DialogTrigger render={<Button variant=\"outline\" />}>\r\n        Edit profile\r\n      </DialogTrigger>\r\n      <DialogPopup className=\"sm:max-w-sm\">\r\n        <DialogHeader>\r\n          <DialogTitle>Edit profile</DialogTitle>\r\n          <DialogDescription>\r\n            Make changes to your profile here. Click save when you&apos;re done.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n        <div className=\"flex flex-col gap-4\">\r\n          <div className=\"\">\r\n            <Label>Name</Label>\r\n            <Input type=\"text\" defaultValue=\"Krishna\" />\r\n          </div>\r\n          <div className=\"\">\r\n            <Label>Username</Label>\r\n            <Input type=\"text\" defaultValue=\"@krishna\" />\r\n          </div>\r\n        </div>\r\n        <DialogFooter>\r\n          <DialogClose render={<Button variant=\"ghost\" />}>Cancel</DialogClose>\r\n          <Button type=\"submit\">Save</Button>\r\n        </DialogFooter>\r\n      </DialogPopup>\r\n    </Dialog>\r\n  );\r\n};\r\n",
      "type": "registry:example"
    }
  ]
}