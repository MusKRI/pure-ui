      "content": "import {\r\n  Tabs,\r\n  TabsList,\r\n  TabsPanel,\r\n  TabsTrigger,\r\n} from \"@/registry/pure-ui/ui/tabs\";\r\n\r\nexport function TabsPanelDemo() {\r\n  return (\r\n    <div className=\"max-w-md mx-auto w-full\">\r\n      <Tabs defaultValue=\"docs\">\r\n        <TabsList>\r\n          <TabsTrigger value=\"docs\">Docs</TabsTrigger>\r\n          <TabsTrigger value=\"components\">Components</TabsTrigger>\r\n          <TabsTrigger value=\"blocks\">Blocks</TabsTrigger>\r\n        </TabsList>\r\n        <div className=\"bg-muted p-4 rounded-lg\">\r\n          <TabsPanel value=\"docs\">\r\n            <div className=\"flex flex-col gap-2\">\r\n              <h3 className=\"text-lg font-medium\">Docs</h3>\r\n              <p className=\"text-sm text-muted-foreground\">\r\n                Docs are a great way to learn about the product and how to use\r\n                it.\r\n              </p>\r\n            </div>\r\n          </TabsPanel>\r\n          <TabsPanel value=\"components\">\r\n            <div className=\"flex flex-col gap-2\">\r\n              <h3 className=\"text-lg font-medium\">Components</h3>\r\n              <p className=\"text-sm text-muted-foreground\">\r\n                Components are a great way to learn about the product and how to\r\n                use it.\r\n              </p>\r\n            </div>\r\n          </TabsPanel>\r\n          <TabsPanel value=\"blocks\">\r\n            <div className=\"flex flex-col gap-2\">\r\n              <h3 className=\"text-lg font-medium\">Blocks</h3>\r\n              <p className=\"text-sm text-muted-foreground\">\r\n                Blocks are a great way to learn about the product and how to use\r\n                it.\r\n              </p>\r\n            </div>\r\n          </TabsPanel>\r\n        </div>\r\n      </Tabs>\r\n    </div>\r\n  );\r\n}\r\n",
      "type": "registry:example"
    }
  ]
}