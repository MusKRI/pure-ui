      "path": "src/registry/pure-ui/examples/dialog/dialog-nested-demo.tsx",
      "content": "import {\r\n  Dialog,\r\n  DialogPopup,\r\n  DialogDescription,\r\n  DialogFooter,\r\n  DialogHeader,\r\n  DialogTitle,\r\n  DialogTrigger,\r\n  DialogClose,\r\n} from \"@/registry/pure-ui/ui/dialog\";\r\n\r\nimport { Button } from \"@/registry/pure-ui/ui/button\";\r\nimport { Input } from \"@/registry/pure-ui/ui/input\";\r\nimport { Label } from \"@/registry/pure-ui/ui/label\";\r\n\r\nexport const DialogNestedDemo = () => {\r\n  return (\r\n    <Dialog>\r\n      <DialogTrigger render={<Button />}>View details</DialogTrigger>\r\n      <DialogPopup showCloseButton={false}>\r\n        <DialogHeader>\r\n          <DialogTitle>Manage team member</DialogTitle>\r\n          <DialogDescription>\r\n            View and manage a user in your team.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n        <div className=\"grid gap-4\">\r\n          <div className=\"grid gap-1\">\r\n            <p className=\"text-sm text-muted-foreground\">Name</p>\r\n            <p className=\"text-sm font-medium\">Krishna</p>\r\n          </div>\r\n          <div className=\"grid gap-1\">\r\n            <p className=\"text-sm text-muted-foreground\">Email</p>\r\n            <p className=\"text-sm font-medium\">krishna@kam-ui.com</p>\r\n          </div>\r\n        </div>\r\n        <DialogFooter>\r\n          <Dialog>\r\n            <DialogTrigger render={<Button variant=\"outline\" />}>\r\n              Edit details\r\n            </DialogTrigger>\r\n            <DialogPopup showCloseButton={false}>\r\n              <DialogHeader>\r\n                <DialogTitle>Edit details</DialogTitle>\r\n                <DialogDescription>\r\n                  Make changes to the member&apos;s information.\r\n                </DialogDescription>\r\n              </DialogHeader>\r\n              <div className=\"flex flex-col gap-4\">\r\n                <div className=\"flex flex-col gap-1\">\r\n                  <Label htmlFor=\"input-type-name\">Name</Label>\r\n                  <Input\r\n                    id=\"input-type-name\"\r\n                    placeholder=\"Krishna\"\r\n                    type=\"text\"\r\n                  />\r\n                </div>\r\n                <div className=\"flex flex-col gap-1\">\r\n                  <Label htmlFor=\"input-type-email\">Email</Label>\r\n                  <Input\r\n                    id=\"input-type-email\"\r\n                    placeholder=\"krishna@example.com\"\r\n                    type=\"text\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <DialogFooter>\r\n                <DialogClose render={<Button variant=\"ghost\" />}>\r\n                  Cancel\r\n                </DialogClose>\r\n                <Button type=\"submit\">Save changes</Button>\r\n              </DialogFooter>\r\n            </DialogPopup>\r\n          </Dialog>\r\n        </DialogFooter>\r\n      </DialogPopup>\r\n    </Dialog>\r\n  );\r\n};\r\n",
      "type": "registry:example"
    }
  ]
}